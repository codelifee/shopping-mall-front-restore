utton>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default AnswerYetQuestionModal;\r\n","C:\\Users\\nayeon\\Desktop\\Web_Project\\shopping-mall-front-restore\\src\\seller\\OrderItems.js",["533","534","535"],"import React, {useState, useEffect} from 'react'\r\nimport axios from '../axios/axios'\r\n\r\nfunction OrderItems({id, name, date, address, status}) {\r\n\r\n    const [newStatus, setNewStatus] = useState({\r\n        order_status: \"배송준비중\",\r\n    })\r\n\r\n    const handleStatus = e => {\r\n        e.preventDefault();\r\n            let id = e.target.id;\r\n\r\n            setNewStatus({\r\n                order_status: e.target.value\r\n            })\r\n\r\n            console.log(newStatus)\r\n\r\n    }\r\n\r\n    const patchOrderStatus = (e) => {\r\n            let id = e.target.id;\r\n\r\n            console.log(id)\r\n            \r\n            axios.patch(`/orders/${id}`, newStatus)\r\n            .then(res => console.log(res))\r\n            .catch(err => console.log(err))\r\n    }\r\n\r\n    useEffect(() => {\r\n        setNewStatus({\r\n            order_status: status\r\n        })        \r\n    }, newStatus)\r\n\r\n    console.log(newStatus)\r\n\r\n    return(\r\n        <>\r\n            <tr>\r\n                <td>{name}</td>\r\n                <td>{address}</td>\r\n                <td>{date}</td>\r\n                <td>\r\n                    <select value={newStatus.order_status} id={id} onChange={handleStatus}>\r\n                        <option value=\"배송완료\">배송완료</option>\r\n                        <option value=\"배송중\">배송중</option>\r\n                        <option value=\"배송시작\">배송시작</option>\r\n                        <option value=\"배송준비중\">배송준비중</option>\r\n                    </select>\r\n                </td>\r\n                <td>\r\n                    <button id={id} onClick={patchOrderStatus}>변경하기</button>\r\n                </td>\r\n            </tr>\r\n        </>\r\n    )\r\n\r\n}\r\n\r\nexp