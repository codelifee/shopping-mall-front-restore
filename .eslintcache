ios/axios';\nimport $ from \"jquery\";\n\nfunction AddCustomer() {\n\n    const [customer, setCustomer] = useState([]);\n\n    useEffect(() => {\n        async function fetchData() {\n            const request = await axios.get(`http://localhost:5000/users/all`)\n            .then(response => \n                setCustomer(response.data)\n            )\n            .catch(error => console.log(error))\n           \n            return request;\n        }\n        fetchData();\n    }, [])\n    console.log(customer)\n\n    const [form, setForm] = useState({\n        user_id : '', //unique\n        user_pwd : '',\n        user_name : '',\n        user_phone : '', //unique\n        user_address: ''\n    })\n\n    const handleChange = e => {\n        e.preventDefault()\n            setForm({\n                ...form,\n                [e.target.name]: e.target.value \n            })\n    }      \n    console.log(form);\n\n    const joinCheck = (e) => {\n        e.preventDefault()\n        if(form.user_id === ''){\n            alert('아이디를 입력하세요.');\n            return false;\n        }\n        if(form.user_pwd === ''){\n            alert('비밀번호를 입력하세요.');\n            return false;\n        }\n        if(form.user_name === ''){\n            alert('이름을 입력하세요.');\n            return false;\n        }\n        if(form.user_phone === ''){\n            alert('전화번호를 입력하세요.');\n            return false;\n        }else{\n            \n        }\n        if(form.user_address === ''){\n            alert('주소를 입력하세요.');\n            return false;\n        }\n    }\n\n    const checkId = () => {\n        let status = true;\n        customer.map((data,i)=>{\n            if(form.user_id === customer[i].user_id){\n                status = false;\n            }\n        })\n        if(status){\n            if(!(/^\\w+([\\.-]?\\w+)*@\\w+([\\.-]?\\w+)*(\\.\\w{2,3})+$/.test(form.user_id))){\n                alert('아이디를 이메일 형식으로 입력하세요.');             \n                return false;\n            }\n            return alert('사용 가능한 아이디입니다.')\n        }else{\n            return alert('사용 불가능한 아이디입니다.')\n        }\n    }\n    const checkPhone = () => {\n        let status = true;\n        customer.map((data,i)=>{\n            if(form.user_phone === customer[i].user_phone){\n                status = false;\n            }\n        })\n        if(status){\n            if(!(/^[0-9]{2,3}-[0-9]{3,4}-[0-9]{4}/.test(form.user_phone))){\n                alert(\"전화번호를 '-' 형식으로 입력하세요.\");\n                return false;\n            }\n            return alert('사용 가능한 전화번호입니다.')\n        }else{\n            return alert('사용 불가능한 전화번호입니다.')\n        }\n    }\n\n    const showForm = (e) => {\n        e.preventDefault()\n        \n        axios.post(`http://localhost:5000/users`, form)\n        .then(alert(\"가입이 완료되었습니다.\"))\n        .catch(err => console.log(err))\n    }\n\n    return (\n        <div className=\"addCustomer\">\n            <div className=\"addCustomer__container\">\n                <div className=\"addCustomer__container__head\">\n                    <h1>Add a New Customer</h1>\n                </div>\n                <form className=\"addCustomer__search\" \n                onSubmit={showForm}>\n                    <div className=\"id\">\n                        <label htmlfor=\"\">Id</label>\n                        <input \n                        type=\"text\" \n                        name=\"user_id\"\n                        onChange={handleChange}\n                        />\n                        <button \n                        type=\"button\"\n                        onClick={checkId}\n                        >\n                            Check Id\n                        </button>\n                    </div>\n                    <div className=\"password\">\n                        <label htmlfor=\"\">Password</label>\n                        <input \n                        type=\"password\" \n                        name=\"user_pwd\"\n                        onChange={handleChange}\n                        />\n                    </div>\n                    <div className=\"name\">\n                        <label htmlfor=\"\">Name</label>\n                        <input \n                        type=\"text\" \n                        name=\"user_name\"\n                        onChange={handleChange}\n                        />\n                    </div>\n                    <div className=\"phone\">\n                        <label htmlfor=\"\">Phone</label>\n                        <input \n                        type=\"text\" \n                        name=\"user_phone\"\n                        onChange={handleChange}\n                        />\n                        <button \n                        type=\"button\"\n                        onClick={checkPhone}\n                        >\n                            Check Phone\n                        </button>\n                    </div>\n                    <div className=\"address\">\n                        <label htmlfor=\"\">Address</label>\n                        <input \n                        type=\"text\" \n                        name=\"user_address\"\n                        onChange={handleChange}\n                        />\n                    </div>\n                    <button \n                    type=\"submit\"\n                    onClick={joinCheck}\n                    >Submit</button>\n                </form>\n            </div>\n        </div>\n    )\n}\n\nexport default AddCustomer","/Users/suminjung/Desktop/Programming/full-stack/shopping-mall-front/src/customer/Navbar.js",["422","423"],"import React, {useState} from 'react'\nimport './Navbar.css'\nimport UpdateCustomer from './UpdateCustomer'\nimport {Link} from 'react-router-dom'\nimport { FaShoppingBag, FaChair,  FaUserCog, FaCaretDown } from \"react-icons/fa\";\nimport Logo from '../img/logo.png'\n\nfunction Navbar() {\n\n    const [order, setOrder] = useState(false);\n    const [user, setUser] = useState(false);\n\n\n    return (\n        <div className=\"navbar\">\n            <Link onClick={() => {window.location.href=\"/\"}}>\n                <div className='navbar__logo'>\n                <img\n                src={Logo} alt=\"\"/>\n                </div>\n            </Link>\n                <div className=\"navbar__links\">\n                    <div className=\"navbar__link\">\n                        <h2>\n                        <FaShoppingBag />\n                            Order\n                            <FaCaretDown \n                        onClick={()=>setOrder(!order)}\n                        className={order ? \"icon-active\" : \"icon\"}\n                        />\n                        </h2>\n                        <div>\n                            {order &&\n                                <Link to=\"/customer/\">\n                                <p>My Orders</p>\n                                </Link>\n                            }\n                        </div>\n                        \n                    </div>\n                    <div className=\"navbar__link\">\n                        <h2>\n                            <FaUserCog />\n                            User\n                            <FaCaretDown \n                        onClick={()=>setUser(!user)}\n                        className={user ? \"icon-active\" : \"icon\"}\n                        />\n                        </h2>\n                        <div>\n                            {user && \n                                <Link to=\"/customer/updatecustomer\">\n                                <p>Update Users</p>\n                                </Link>\n                            }\n                        </div>\n                    </div>\n                </div>\n                \n            </div>\n    )\n}\n\nexport default Navbar\n","/Users/suminjung/Desktop/Programming/full-stack/shopping-mall-front/src/custo